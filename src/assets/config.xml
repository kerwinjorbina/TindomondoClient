<widget id="com.tindomondo" version="0.1.0">
  <name>Tindomondo</name>
  <description>
    Tindomondo
  </description>
  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" />
  <access origin="*" />

  <icon src="assets/images/icon/ldpi.png" platform="android" qualifier="ldpi" />
  <icon src="assets/images/icon/mdpi.png" platform="android" qualifier="mdpi" />
  <icon src="assets/images/icon/hdpi.png" platform="android" qualifier="hdpi" />
  <icon src="assets/images/icon/xhdpi.png" platform="android" qualifier="xhdpi" />
  <icon src="assets/images/icon/xxhdpi.png" platform="android" qualifier="xxhdpi" />
  <icon src="assets/images/icon/fr-xxhdpi.png" platform="android" qualifier="fr-xxhdpi" />

  <splash src="assets/images/splash/ldpi.png" platform="android" qualifier="ldpi" />
  <splash src="assets/images/splash/mdpi.png" platform="android" qualifier="mdpi" />
  <splash src="assets/images/splash/hdpi.png" platform="android" qualifier="hdpi" />
  <splash src="assets/images/splash/xhdpi.png" platform="android" qualifier="xhdpi" />
  <splash src="assets/images/splash/fr-xhdpi.png" platform="android" qualifier="fr-xhdpi" />
  <splash src="assets/images/splash/portrait-xxhdpi.png" platform="android" qualifier="port-xxhdpi" />
  <splash src="assets/images/splash/landscape-xxhdpi.png" platform="android" qualifier="land-xxhdpi" />

</widget>
